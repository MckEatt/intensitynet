citHeader("To cite intensitynet in publications use:")

citEntry(entry= "Manual",
         title = "{intensitynet}: Network intensity function for planar networks",
         author = person("Matthias Eckardt"),
         year = 2016,
         url = "https://github.com/MckEatt/intensitynet",

  textVersion = paste("Matthias Eckardt (2016)",
                "intensitynet: Network intensity function for planar networks",   note, "."))
                
                
  citEntry(entry = "Article",
  title        = "Point patterns occurring on complex structures in space and space-time: An alternative network approach",
  author       = personList(as.person("Matthias Eckardt"),
                   as.person("Jorge Mateu")),
  journal      = "arXiv",
  year         = "2016",
  url          = "https://arxiv.org/abs/1607.06288",

  textVersion  =  paste("Matthias Eckardt and Jorge Mateu (2016).",
        "Point patterns occurring on complex structures in space and space-time: An alternative network approach.",
        "https://arxiv.org/abs/1607.06288"))              
